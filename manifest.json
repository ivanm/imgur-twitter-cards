{
	"name": "Imgur Twitter cards",
	"version": "1.0",
	"description": "Load imgur images from twitter cards",
	"content_scripts": [
		{
			"matches": [ "https://twitter.com/*" ],
			"js": ["jquery.min.js", "imgurscript.js"]
		}
	],
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"48": "icon48.png"
	},
	"manifest_version": 2
}
